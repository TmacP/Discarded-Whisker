-- card.script

go.property("name", hash(""))

local speed = -240


function init(self)
	-- Initialization code here
	msg.post("#sprite", "play_animation", { id = self.name })
	print(self.name)

end



function update(self, dt)
	local p = go.get_position()
	p.x = p.x + speed * dt
	if p.x < -32 then
		go.delete()
	end
	go.set_position(p)
end
